        "def run_model(text_clf_classifier):\n",
        "  print(text_clf_classifier)\n",
        "  classifier_model = text_clf_classifier.fit(X_train_class,Y_train_class)\n",
        "  print(np.mean(classifier_model.predict(X_test_class)== Y_test_class))"
